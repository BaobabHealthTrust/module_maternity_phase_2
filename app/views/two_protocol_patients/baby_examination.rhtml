<script type='text/javascript' language='javascript'>
<!--
	tt_cancel_show = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	tt_cancel_destination = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	</script>

<form method='post' action='/encounters/create'>
	<input type='hidden' name='program' value='UNDER 5 PROGRAM' />
	<input type='hidden' name='encounter_type' value='PHYSICAL EXAMINATION BABY ' />
	<input type='hidden' name='patient_id' value='<%= @patient.id %>' />
	<input type='hidden' name='user_id' value='<%= session[:user_id] %>' />
	<table>
		<tr>
			<td>
				Condition at Admission 
			</td>
			<td>
<select id='1.1.2'   field_type='text'  name='concept[Condition of baby at admission]'  helpText='Condition of baby at admission'  pos='1' >
<option></option>
<option value='Stable'>Stable</option>
 <option value='Critical'>Critical</option>
 <option value='Dead'>Dead</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Temperature 
			</td>
			<td>
<input type='text' id='1.1.3'  name='concept[TEMPERATURE]'   absoluteMin='0'  min='36'  field_type='number'  absoluteMax='100'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  helpText='Baby temperature (<sup>o</sup>C)'  pos='2'  max='38'  tt_onLoad='attribute("1.1.3*validationRule*([0-9]+\\.[0-9])|Unknown$")'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"' />
			</td>
		</tr>
		<tr>
			<td>
				Respiratory Rate 
			</td>
			<td>
<input type='text' id='1.1.4'  name='concept[RESPIRATORY RATE]'   min='0'  field_type='number'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  helpText='Respiratory rate'  pos='3'  max='80'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"'  units='bpm' />
			</td>
		</tr>
		<tr>
			<td>
				Heart Rate 
			</td>
			<td>
<input type='text' id='1.1.5'  name='concept[HEART RATE]'   min='20'  field_type='number'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  helpText='Heart rate'  pos='4'  max='60'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"'  units='bpm' />
			</td>
		</tr>
		<tr>
			<td>
				Baby Weight (grams) 
			</td>
			<td>
<input type='text' id='1.1.6'  name='concept[WEIGHT]' helpText='Baby Weight (grams) '  min='2500'  field_type='number'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  pos='5'  max='5000'  tt_onLoad='attribute("1.1.5*validationRule*([0-9]+\\.[0-9])|Unknown$")'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"' />
			</td>
		</tr>
		<tr>
			<td>
				Abdomen 
			</td>
			<td>
<select id='1.1.7' name='concept[ABDOMEN]'  field_type='text'  helpText='Abdomen'  pos='6'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"' >
<option></option>
<option value='Distended'>Distended</option>
 <option value='Soft'>Soft</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Cord Tied 
			</td>
			<td>
<select id='1.1.8' name='concept[CORD TIED]'  field_type='text'  helpText='Cord Tied'  pos='7'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"' >
<option></option>
<option value='Yes'>Yes</option>
 <option value='No'>No</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Any Abnormalities 
			</td>
			<td>
<select id='1.1.9' name='concept[Any abnormalities]'  field_type='text'  helpText='Any abnormalities'  pos='8'  disabled='disabled'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"' >
<option></option>
<option value='Yes'>Yes</option>
 <option value='No'>No</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Specify Abnormalities 
			</td>
			<td>
<input type='text' id='1.1.10'  name='concept[SPECIFY]'   field_type='text'  helpText='Specify abnormality(s)'  pos='9'  condition='__$("1.1.2").value.trim().toUpperCase() != "DEAD"; __$("1.1.9").value.trim().toUpperCase() == "YES"' />
			</td>
		</tr>
	</table>
</form>
