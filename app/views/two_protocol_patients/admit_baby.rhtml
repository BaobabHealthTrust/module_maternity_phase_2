<link href='/stylesheets/generalCondition.css' type='text/css' rel='stylesheet' />

<script type='text/javascript' src='/javascripts/generalCondition.js' ></script>

<script type='text/javascript' language='javascript'>
<!--
	tt_cancel_show = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	tt_cancel_destination = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	</script>

<form method='post' action='/encounters/create'>
	<input type='hidden' name='program' value='UNDER 5 PROGRAM' />
	<input type='hidden' name='encounter_type' value='Admit patient ' />
	<input type='hidden' name='patient_id' value='<%= @patient.id %>' />
	<input type='hidden' name='user_id' value='<%= session[:user_id] %>' />
	<table>
		<tr>
			<td>
				Admission date 
			</td>
			<td>
<input type='text' id='1.3'  name='concept[Admission date ]' helpText='Admission date '  tt_onLoad='$("Unknown").style.display = "none"'  pos='2'  min='<%= @patient.valid_delivery_time.to_date rescue nil%>'  field_type='date' />
			</td>
		</tr>
		<tr>
			<td>
				Admission time 
			</td>
			<td>
<input type='text' id='1.4'  name='concept[Admission time ]' helpText='Admission time '  pos='3'  field_type='advancedTime' />
			</td>
		</tr>
		<tr>
			<td>
				Baby weight at admission (grams) 
			</td>
			<td>
<input type='text' id='1.5'  name='concept[Baby weight at admission]' helpText='Baby weight at admission (grams) '  absoluteMax='10000'  max='5000'  pos='4'  min='2000'  field_type='number'  tt_pageStyleClass='NumbersOnlyWithDecimal'  absoluteMin='100' />
			</td>
		</tr>
		<tr>
			<td>
				Baby temperature (<sup>o</sup>C) 
			</td>
			<td>
<input type='text' id='1.6'  name='concept[temperature]' helpText='Baby temperature (<sup>o</sup>C) '  absoluteMax='70'  max='38'  pos='5'  min='36'  field_type='number'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  absoluteMin='0' />
			</td>
		</tr>
		<tr>
			<td>
				Reason for admission 
			</td>
			<td>
<input type='text' id='1.7'   helpText='Reason for admission '  ajaxUrl='/encounters/diagnoses?set=nursery_admission_diagnoses&search_string='  name='concept[Reason for admission][]'  pos='6' />
			</td>
		</tr>
		<tr>
			<td>
				Next reason for admission (Optional) 
			</td>
			<td>
<input type='text' id='1.8'   helpText='Next reason for admission (Optional) '  ajaxUrl='/encounters/diagnoses?set=nursery_admission_diagnoses&search_string='  name='concept[Reason for admission][]'  pos='7'  optional='true' />
			</td>
		</tr>
		<tr>
			<td>
				Blood sugar (mg/dL) 
			</td>
			<td>
<input type='text' id='1.9'   helpText='Blood sugar (mg/dL) '  max='150'  name='concept[Blood sugar]'  pos='8'  field_type='number'  min='30' />
			</td>
		</tr>
		<tr>
			<td>
				Vitamin K given 
			</td>
			<td>
<select id='1.10'  helpText='Vitamin K given ' name='concept[Vitamin K given?]'  pos='9' >
<option></option>
<option value='Yes'>Yes</option>
 <option value='No'>No</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Suckling reflex available? 
			</td>
			<td>
<select id='1.11'  helpText='Suckling reflex available? ' name='concept[Observation on suckling reflex]'  pos='10' >
<option></option>
<option value='Yes'>Yes</option>
 <option value='No'>No</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				General Condition 
			</td>
			<td>
<input type='text' id='1.12'  name='concept[General condition]'   tt_onLoad='showGeneralConditionControl(); __$("keyboard").style.display = "none"'  pos='11'  tt_pageStyleClass='NoKeyboard'  helpText='General condition of baby' />
			</td>
		</tr>
		<tr>
			<td>
				Plan of action 
			</td>
			<td>
<textarea type='text' id='1.13'   helpText='Plan of action '  name='concept[Plan]'  pos='12'  field_type='textarea' > </textarea>
			</td>
		</tr>
		<tr>
			<td>
				Comments 
			</td>
			<td>
<textarea type='text' id='1.14'   helpText='Comments '  name='concept[clinician notes]'  pos='13'  field_type='textarea'  optional='true' > </textarea>
			</td>
		</tr>
		<tr>
			<td>
				Next URL 
			</td>
			<td>
<input  id='1.15'   helpText='Next URL '  name='next_url'  pos='14'  type='hidden'  value='/patients/baby_admissions_note?identifier=<%= @patient.national_id%>&user_id=<%= @user.id %>&patient_id=<%= @patient.id %>' />
			</td>
		</tr>
	</table>
</form>
