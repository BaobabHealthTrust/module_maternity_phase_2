<script type='text/javascript' src='/javascripts/vitals.js' ></script>

<script type='text/javascript' language='javascript'>
<!--
	tt_cancel_show = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	tt_cancel_destination = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	</script>

<form method='post' action='/encounters/create'>
	<input type='hidden' name='program' value='MATERNITY PROGRAM' />
	<input type='hidden' name='encounter_type' value='Vitals ' />
	<input type='hidden' name='patient_id' value='<%= @patient.id %>' />
	<input type='hidden' name='user_id' value='<%= session[:user_id] %>' />
	<table>
		<tr>
			<td>
				Systolic Blood Pressure 
			</td>
			<td>
<input type='text' id='1.1.1'  name='concept[SYSTOLIC BLOOD PRESSURE]' helpText='Systolic Blood Pressure '  tt_onUnLoad='window.clearInterval(timedEvent); temp = ""; bpOn = false'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  absoluteMin='20'  min='90'  field_type='number'  absoluteMax='250'  max='140'  units='mm Hg'  pos='0'  tt_onLoad='bpOn = 1; calculateBP("1.1.1*1.1.2*1"); bpAlerts()'  allowFreeText='true' />
			</td>
		</tr>
		<tr>
			<td>
				Diastolic Blood Pressure 
			</td>
			<td>
<input type='text' id='1.1.2'  name='concept[DIASTOLIC BLOOD PRESSURE]' helpText='Diastolic Blood Pressure '  tt_onUnLoad='window.clearInterval(timedEvent); temp = ""; bpOn = false'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  absoluteMin='35'  min='60'  field_type='number'  absoluteMax='135'  max='90'  units='mm Hg'  pos='1'  tt_onLoad='bpOn = 2; calculateBP("1.1.1*1.1.2*2"); bpAlerts()'  allowFreeText='true' />
			</td>
		</tr>
		<tr>
			<td>
				Pulse (Beats Per Minute)
			</td>
			<td>
<input type='text' id='1.1.3'  name='concept[PULSE]' helpText='Pulse (Beats Per Minute)'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  absoluteMin='0'  min='50'  field_type='number'  absoluteMax='160'  max='120'  units='bpm'  pos='3'  tt_onLoad='attribute("1.1.3*validationRule*([0-9]+)|Unknown$")' />
			</td>
		</tr>
		<tr>
			<td>
				Respiration (Breaths Per Minute) 
			</td>
			<td>
<input type='text' id='1.1.4'  name='concept[RESPIRATION]' helpText='Respiration (Breaths Per Minute) '  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  absoluteMin='0'  min='12'  field_type='number'  absoluteMax='200'  max='20'  units='bpm'  pos='4'  tt_onLoad='attribute("1.1.4*validationRule*([0-9]+)|Unknown$")' />
			</td>
		</tr>
		<tr>
			<td>
				Temperature 
			</td>
			<td>
<input type='text' id='1.1.5'  name='concept[TEMPERATURE]'   helpText='Temperature (<sup>o</sup>C)'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  absoluteMin='0'  min='36.5'  field_type='number'  absoluteMax='100'  max='37.5'  pos='5'  tt_onLoad='attribute("1.1.5*validationRule*([0-9]+)|Unknown$")' />
			</td>
		</tr>
	</table>
</form>
