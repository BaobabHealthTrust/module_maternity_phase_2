<script type='text/javascript' src='/touchscreentoolkit/lib/javascripts/vitals.js' ></script>

<script type='text/javascript' language='javascript'>
<!--
	tt_cancel_show = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	tt_cancel_destination = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	</script>

<form method='post' action='/encounters/create'>
	<input type='hidden' name='program' value='MATERNITY PROGRAM' />
	<input type='hidden' name='encounter_type' value='Vitals ' />
	<input type='hidden' name='patient_id' value='<%= @patient.id %>' />
	<input type='hidden' name='user_id' value='<%= session[:user_id] %>' />
	<table>
		<tr>
			<td>
				Systolic Blood Pressure 
			</td>
			<td>
<input type='text' id='1.1.1'  name='concept[SYSTOLIC BLOOD PRESSURE]' helpText='Systolic Blood Pressure '  absoluteMin='20'  absoluteMax='250'  max='140'  allowFreeText='true'  min='90'  units='mm Hg'  tt_onLoad='bpOn = 1; calculateBP("1.1.1*1.1.2*1"); bpAlerts(); attribute("1.1.1*validationRule*([0-9]+\\.[0-9])|Unknown$"); attribute("1.1.1*validationMessage*You must enter a decimal between 0 and 9 (for example 36<b>.6</b>)")'  tt_onUnLoad='window.clearInterval(timedEvent); temp = ""; bpOn = false'  pos='0'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  field_type='number' />
			</td>
		</tr>
		<tr>
			<td>
				Diastolic Blood Pressure 
			</td>
			<td>
<input type='text' id='1.1.2'  name='concept[DIASTOLIC BLOOD PRESSURE]' helpText='Diastolic Blood Pressure '  absoluteMin='35'  absoluteMax='135'  max='90'  allowFreeText='true'  min='60'  units='mm Hg'  tt_onLoad='bpOn = 2; calculateBP("1.1.1*1.1.2*2"); bpAlerts(); attribute("1.1.2*validationRule*^([0-9]+)|Unknown$")'  tt_onUnLoad='window.clearInterval(timedEvent); temp = ""; bpOn = false'  pos='1'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  field_type='number' />
			</td>
		</tr>
		<tr>
			<td>
				Pulse (Beats Per Minute)
			</td>
			<td>
<input type='text' id='1.1.3'  name='concept[PULSE]' helpText='Pulse (Beats Per Minute)'  absoluteMin='0'  absoluteMax='160'  max='120'  min='50'  units='bpm'  tt_onLoad='attribute("1.1.3*validationRule*([0-9]+)|Unknown$")'  pos='3'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  field_type='number' />
			</td>
		</tr>
		<tr>
			<td>
				Respiration (Breaths Per Minute) 
			</td>
			<td>
<input type='text' id='1.1.4'  name='concept[RESPIRATION]' helpText='Respiration (Breaths Per Minute) '  absoluteMin='0'  absoluteMax='200'  max='20'  min='12'  units='bpm'  tt_onLoad='attribute("1.1.4*validationRule*([0-9]+)|Unknown$")'  pos='4'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  field_type='number' />
			</td>
		</tr>
		<tr>
			<td>
				Temperature 
			</td>
			<td>
<input type='text' id='1.1.5'  name='concept[TEMPERATURE]'   absoluteMin='0'  absoluteMax='100'  max='37.5'  min='36.5'  helpText='Temperature (<sup>o</sup>C)'  tt_onLoad='attribute("1.1.5*validationRule*([0-9]+)|Unknown$")'  pos='5'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  field_type='number' />
			</td>
		</tr>
		<tr>
			<td>
				Fetal Heart Rate 
			</td>
			<td>
<input type='text' id='1.1.6'  name='concept[FETAL HEART RATE]' helpText='Fetal Heart Rate '  absoluteMin='0'  absoluteMax='200'  pos='6'  tt_pageStyleClass='Numeric NumbersOnlyWithDecimal'  field_type='number' />
			</td>
		</tr>
		<tr>
			<td>
				Next URL 
			</td>
			<td>
<input  id='1.1.7'   helpText='Next URL '  type='hidden'  name='ret'  value='ante-natal'  pos='7' />
			</td>
		</tr>
	</table>
</form>
