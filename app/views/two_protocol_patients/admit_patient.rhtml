<script type='text/javascript' language='javascript'>
<!--
	tt_cancel_show = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	tt_cancel_destination = '/patients/show/<%= @patient.id %>?user_id=<%= session[:user_id] %>';
	</script>

<form method='post' action='/encounters/create'>
	<input type='hidden' name='program' value='MATERNITY PROGRAM' />
	<input type='hidden' name='encounter_type' value='Admit patient ' />
	<input type='hidden' name='patient_id' value='<%= @patient.id %>' />
	<input type='hidden' name='user_id' value='<%= session[:user_id] %>' />
	<table>
		<tr>
			<td>
				Gravida 
			</td>
			<td>
<input type='text' id='1.1'  name='concept[Gravida ]' helpText='Gravida '  pos='0'  tt_pageStyleClass='NumbersOnly'  absoluteMax='20'  max='10'  field_type='number'  absoluteMin='1' />
			</td>
		</tr>
		<tr>
			<td>
				Parity 
			</td>
			<td>
<input type='text' id='1.2'  name='concept[Parity ]' helpText='Parity '  pos='1'  tt_pageStyleClass='NumbersOnly'  absoluteMax='20'  max='10'  field_type='number'  condition='__$("1.1").value > 1'  absoluteMin='1' />
			</td>
		</tr>
		<tr>
			<td>
				Gestation weeks 
			</td>
			<td>
<input type='text' id='1.3'  name='concept[Gestation weeks ]' helpText='Gestation weeks '  pos='2'  tt_pageStyleClass='NumbersOnly'  absoluteMax='45'  max='42'  min='36'  field_type='number'  absoluteMin='1' />
			</td>
		</tr>
		<tr>
			<td>
				Previous HIV Test Status From Before Current Facility Visit 
			</td>
			<td>
<select id='1.4' name='concept[Previous HIV Test Status From Before Current Facility Visit ]' helpText='Previous HIV Test Status From Before Current Facility Visit ' pos='3' >
<option></option>
<option value='Unknown'>Unknown</option>
 <option value='Reactive'>Reactive</option>
 <option value='Non-reactive less than 3 months'>Non-reactive less than 3 months</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				HIV status 
			</td>
			<td>
<select id='1.4.3.1' name='concept[HIV status ]' helpText='HIV status ' condition='__$("1.4").value == "Unknown"'  pos='4' >
<option></option>
<option value='Reactive'>Reactive</option>
 <option value='Not done'>Not done</option>
 <option value='Non-reactive'>Non-reactive</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				On ART 
			</td>
			<td>
<select id='1.5' name='concept[On ART ]' helpText='On ART ' pos='5'  condition='((__$("1.4.3.1").value.toLowerCase().trim() == "reactive")  || (__$("1.4").value.toLowerCase().trim() == "reactive"))' >
<option></option>
<option value='Yes'>Yes</option>
 <option value='No'>No</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Date antiretrovirals started 
			</td>
			<td>
<select id='1.5.2.1' name='concept[Date antiretrovirals started ]'  condition='__$("1.5").value == "Yes"'  helpText='When was ART started?'  pos='6' >
<option></option>
<option value='On ART during labour'>On ART during labour</option>
 <option value='On ART since third trimester'>On ART since third trimester</option>
 <option value='On ART since first or second trimester'>On ART since first or second trimester</option>
 <option value='On ART before pregnancy'>On ART before pregnancy</option>
 </select>
			</td>
		</tr>
		<tr>
			<td>
				Mother ART registration number 
			</td>
			<td>
<input type='text' id='1.5.2.2'  name='concept[Mother ART registration number ]' helpText='Mother ART registration number '  condition='__$("1.5").value == "Yes"'  pos='7' />
			</td>
		</tr>
	</table>
</form>
